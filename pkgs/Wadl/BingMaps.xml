  <resources siena:authenticationProviderHref="#PowerAppAuth" base="https://india-001.azure-apim.net/apim/bingmaps">
    <resource path="/{connectionId}/REST/V1/Routes/Driving">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <method siena:requiresAuthentication="true" name="GET" id="GetRoute" visibility="advanced" siena:isDeprecated="false">
        <doc title="Get route&#xA;Calculate a route using waypoints." />
        <request>
          <param style="query" name="wp.0" type="xs:string" required="true">
            <doc title="Specified as a point, landmark, or address. (47.610,-122.107 or Seattle, WA)" />
          </param>
          <param style="query" name="wp.1" type="xs:string" required="true">
            <doc title="Specified as a point, landmark, or address. (47.610,-122.107 or Seattle, WA)" />
          </param>
          <param style="query" name="avoid" type="xs:string">
            <doc title="A comma-separated list of values from the following list (highways, tolls, minimizeHighways, minimizeTolls)." />
          </param>
          <param style="query" name="optimize" type="xs:string">
            <doc title="Specifies what parameters to use to optimize the route." />
            <option value="distance" />
            <option value="time " />
            <option value="timeWithTraffic" />
            <option value="timeAvoidClosure" />
          </param>
          <param style="query" name="distanceUnit" type="xs:string">
            <doc title="The units to use for distance in the response. Default: Kilometer" />
            <option value="Mile" />
            <option value="Kilometer" />
          </param>
          <param style="query" name="travelMode" type="xs:string">
            <doc title="The mode of travel for the route. Default: Driving" />
            <option value="Driving" />
            <option value="Walking" />
            <option value="Transit" />
          </param>
          <param style="query" name="dateTime" type="xs:string">
            <doc title="Required when the travel mode is transit. Identifies the desired transit time, such as arrival time or departure time." />
          </param>
          <param style="query" name="timeType" type="xs:string">
            <doc title="Required when the travel mode is transit. Specifies how to interpret the transit date-time value." />
            <option value="Arrival" />
            <option value="Departure" />
            <option value="LastAvailable" />
          </param>
        </request>
        <response>
          <representation mediaType="application/json" element="service:GetRoute_Response" />
        </response>
      </method>
    </resource>
    <resource path="/{connectionId}/REST/v1/Locations/pointPlaceHolder">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <method siena:requiresAuthentication="true" name="GET" id="GetLocationByPoint" visibility="advanced" siena:isDeprecated="false">
        <doc title="Get location by point&#xA;Get the location information associated with latitude and longitude coordinates." />
        <request>
          <param style="query" name="latitude" type="xs:double" required="true">
            <doc title="The latitude coordinate you want to reverse geocode. Example: 47.64054" />
          </param>
          <param style="query" name="longitude" type="xs:double" required="true">
            <doc title="The longitude coordinate you want to reverse geocode. Example: -122.12934" />
          </param>
          <param style="query" name="includeEntityTypes" type="xs:string">
            <doc title="The entity types that you want to return in the response." />
          </param>
          <param style="query" name="includeNeighborhood" type="xs:boolean" default="true">
            <doc title="A value determining whether or not to include the neighborhood in the response when it is available. 0 or 1" />
          </param>
          <param style="query" name="include" type="xs:boolean" default="true">
            <doc title="A value determining whether or not to include the two-letter ISO country code. ciso2" />
          </param>
        </request>
        <response>
          <representation mediaType="application/json" element="service:GetLocation_Response" />
        </response>
      </method>
    </resource>
    <resource path="/{connectionId}/REST/v1/Locations">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <method siena:requiresAuthentication="true" name="GET" id="GetLocationByAddress" visibility="advanced" siena:isDeprecated="false">
        <doc title="Get location by address&#xA;Get the location information associated with an address. If no location is found, an empty result will be returned." />
        <request>
          <param style="query" name="addressLine" type="xs:string">
            <doc title="A string specifying the street line of an address (not including city, country region, etc)." />
          </param>
          <param style="query" name="locality" type="xs:string">
            <doc title="A string that contains the locality, such as a US city." />
          </param>
          <param style="query" name="adminDistrict" type="xs:string">
            <doc title="A string that contains a subdivision, such as the abbreviation of a US state." />
          </param>
          <param style="query" name="postalCode" type="xs:string">
            <doc title="A string that contains the postal code, such as a US ZIP Code." />
          </param>
          <param style="query" name="countryRegion" type="xs:string">
            <doc title="A string specifying the ISO country code." />
          </param>
        </request>
        <response>
          <representation mediaType="application/json" element="service:GetLocation_Response" />
        </response>
      </method>
    </resource>
