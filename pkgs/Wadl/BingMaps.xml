      </param>
      <method siena:requiresAuthentication="true" name="GET" id="GetMap" visibility="advanced" siena:isDeprecated="true" siena:family="GetMap" siena:revision="1" siena:status="Preview">
        <doc title="Get static map&#xA;Get static map." />
        <request>
          <param style="query" name="latitude" type="xs:string" required="true">
            <doc title="The latitude coordinate you want to reverse geocode. Example: 47.64054" />
          </param>
          <param style="query" name="longitude" type="xs:string" required="true">
            <doc title="The longitude coordinate you want to reverse geocode. Example: -122.12934" />
          </param>
          <param style="query" name="format" type="xs:string">
            <doc title="The image format to use for the static map." />
            <option value="gif" />
            <option value="jpeg" />
            <option value="png" />
          </param>
          <param style="query" name="mapSize" type="xs:string">
            <doc title="The width and height in pixels of the static map output. Example: 1000,600." />
          </param>
          <param style="query" name="pushpin" type="xs:string">
            <doc title="Point value (latitude and longitude) with options to add a label of up to three (3) characters and to specify an icon style. Example: 47.610,-122.107;5;P10 (more information https://aka.ms/u8b8aq)." />
          </param>
        </request>
        <response>
          <representation mediaType="application/octet-stream" siena:dtype="image" />
        </response>
      </method>
    </resource>
